{
  "name": "weibo utils",
  "description": "一些微博开发工具",
  "version": "0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/weibo.png",
      "32": "/images/weibo.png",
      "48": "/images/weibo.png",
      "128": "/images/weibo.png"
    }
  },
  "icons": {
    "16": "/images/weibo.png",
    "32": "/images/weibo.png",
    "48": "/images/weibo.png",
    "128": "/images/weibo.png"
  }
}